            <stringProp name="BeanShellSampler.query">import com.jcraft.jsch.*;


String host = &quot;10.250.46.137&quot;;
int port = 22;
String user = &quot;administrator&quot;;
String passwd = &quot;admin1234&quot;;

JSch jsch = null; 
Session sess = null;
Channel channel = null;
ChannelExec exec = null;
InputStream in = null;
OutputStream out = null;


try{
    	log.info(&quot;login&quot;);
    	jsch = new JSch();

	sess = jsch.getSession(user, host, port);
	sess.setPassword(passwd);
	sess.setConfig(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;);
	sess.connect();

	exec = (ChannelExec)sess.openChannel(&quot;exec&quot;);

	in = exec.getInputStream();
	out = exec.getOutputStream();
	exec.setErrStream(System.err);

	exec.connect();
	
	
	
    	
}catch(Exception ex){
	log.error(&quot;Fail to login to remote SSH shell&quot;, ex);
	
}finally{

	if(exec != null){
		exec.disconnect();
	}

	if(sess != null){
		sess.disconnect();
	}
}

</stringProp>
